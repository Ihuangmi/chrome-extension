{
  "name": "chrome-extension",
  "version": "1.0.0",
  "description": "chrome-extension",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --config config/webpack.dev.js --watch",
    "build": "webpack --config config/webpack.prod.js",
    "clean": "rimraf dist",
    "test": "npx jest",
    "style": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^4.20.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.158",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-plugin-import": "^1.13.5",
    "copy-webpack-plugin": "^9.0.1",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.0",
    "typescript": "^4.4.3 ",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.0.0",
    "windicss-webpack-plugin": "^1.7.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  }
}
